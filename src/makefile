
# Compiler flags
CFLAGS=-g -O3

#Targets to build
all:main 

# Include file dependedncies
tokenizer.o: tokenizer.h
main.o: tokenizer.h history.h
history.o: history.h


# Build main program
main: tokenizer.o history.o main.o
	cc -o $@ $^ #(CFLAGS)

# Delete files generated by compilation
clean:
	rm -f *.o main 
